{
  "installCommand": "npm install && npm --prefix cpulse-frontend install",
  "buildCommand": "npx tsc && npm --prefix cpulse-frontend run build",
  "outputDirectory": "cpulse-frontend/build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}